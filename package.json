{
  "name": "es-module-path-case-check",
  "version": "0.0.1",
  "description": "Es module path case check tool.",
  "bin": {
    "case-detection": "lib/cli.js"
  },
  "dependencies": {
    "@babel/parser": "^7.4.2",
    "colors": "^1.3.3",
    "glob": "^7.1.3",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
